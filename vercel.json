{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.0.0",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS"],
      "dest": "/api/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://f1-indol-theta.vercel.app",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}